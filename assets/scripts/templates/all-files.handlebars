<table class="centered striped bordered file">
  <thead>
    <tr>
      <th data-field="name">File Name</th>
      <th data-field="modified">Last Modified</th>
      <th data-field="path">Path</th>
      <th>
    </tr>
  </thead>
  <tbody>
    {{#files}}
      <tr>
        <td>{{this.name}}</td>
        <td>{{this.updated_at}}</td>
        <td>{{this.path}}</td>
        <td><a id="edit-tags" class="waves-effect waves-light btn">Edit Tags</a></td>
        <td><a id="delete-file" class="waves-effect waves-light btn">Delete File</a></td>
      </tr>
    {{/files}}
  </tbody>
</table>
